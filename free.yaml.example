AndroidVersionName: "1.0.3"
AndroidVersionCode: 6
AndroidVersionPage: "https://www.pgyer.com/freeim"
AndroidVersionDesc: "修复聊天会话错乱BUG。
                     修复其他已知BUG"

# 运行环境 dev(开发环境) |　pre(灰度环境) |  prod(生产环境)
RunSetting: "dev"
MySQL: "root:root@tcp(127.0.0.1:3306)/free_im?charset=utf8mb4"
RedisIP: "127.0.0.1:6379"
RedisAuth: ""

# logic 模块配置
LogicConf:
  RPCListenAddr: ":5000"
  ConnRPCAddrs: "127.0.0.1:5001"

# conn 模块配置
ConnectConf:
  TCPListenAddr: ":2000"
  WSListenAddr:  ":2001"
  RPCListenAddr: ":5001"
  LogicRPCAddrs: "127.0.0.1:5000"

# http 模块配置
HttpConf:
  HttpListenAddr: ":8080"
  RPCListenAddr: ":5003"
  LogicRPCAddrs: "127.0.0.1:5000"


# 阿里云短信
AliYunSmsAccessKeyID: ""
AliYunSmsAccessKeySecret: ""

# 七牛
QiniuAccessKey: ""
QiniuSecretKey: ""

# QQ互联APPID
QQAuthAppID: ""